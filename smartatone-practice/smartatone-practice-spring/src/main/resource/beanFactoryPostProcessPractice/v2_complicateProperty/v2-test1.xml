<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd
       ">


    <bean id="consumerBeam" class="com.smartatone.BeanFactoryPostProcessPractice.v2_complicateProperty.ConsumerBean"
          init-method="start">
        <property name="properties" ref="myProperties"/>
    </bean>

    <bean id="myProperties" class="java.util.Properties">
        <constructor-arg>
            <props>
                <prop key="name">java</prop>
            </props>
        </constructor-arg>
    </bean>

    <!--å¤„ç†ç±»-->
    <bean id="myBeanPostProcessor"
          class="com.smartatone.BeanFactoryPostProcessPractice.v2_complicateProperty.test1.MyBeanPostProcessor"/>

</beans>