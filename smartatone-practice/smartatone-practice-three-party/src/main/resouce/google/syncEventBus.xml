<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd
       ">

    <bean id="ob1" class="com.smartatone.google.eventBus.syncEventBus.DataObserver1"/>
    <bean id="ob2" class="com.smartatone.google.eventBus.syncEventBus.DataObserver2"/>

    <bean id="eventBus" class="com.google.common.eventbus.EventBus"/>

    <bean id="eventBusCenter" class="com.smartatone.google.eventBus.syncEventBus.EventBusCenter">
        <property name="eventBus" ref="eventBus"/>
    </bean>

</beans>